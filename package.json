{
  "name": "codiff",
  "displayName": "CoDiff",
  "icon": "media/icon.png",
  "repository": {
		"type": "git",
		"url": "https://github.com/cotnw/codiff"
	},
  "description": "Have live access to all your teammates' local changes, get notified when conflicts occur with teammates.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.55.0"
  },
  "publisher": "bar9",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:codiff-sidebar"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codiff-sidebar-view",
          "title": "CoDiff",
          "icon": "media/sidebar-icon.png"
        }
      ]
    },
    "views": {
      "codiff-sidebar-view": [
        {
          "type": "webview",
          "id": "codiff-sidebar",
          "name": "CoDiff",
          "icon": "media/sidebar-icon.png",
          "contextualTitle": "CoDiff"
        }
      ]
    },
    "commands": [
      {
        "command": "codiff.clearGlobalStorage",
        "category": "CoDiff",
        "title": "Clear global storage"
      },
      {
        "command": "codiff.start",
        "category": "CoDiff",
        "title": "Start conflict detection for the current branch and repository"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@types/polka": "^0.5.2",
    "axios": "^0.21.1",
    "polka": "^0.5.2",
    "socket.io-client": "^4.0.1",
    "tmp": "^0.2.1",
    "utf-8-validate": "^5.0.2"
  }
}
